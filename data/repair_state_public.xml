<odoo>
    <record id="tech_repair_state_pub_new" model="tech.repair.state.public">
        <field name="name">Repair Received</field>
        <field name="sequence">1</field>
    </record>
    <record id="tech_repair_state_pub_in_progress" model="tech.repair.state.public">
        <field name="name">In Repair</field>
        <field name="sequence">2</field>
    </record>
    <record id="tech_repair_state_pub_waiting_part" model="tech.repair.state.public">
