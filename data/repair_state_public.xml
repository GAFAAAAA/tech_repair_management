        <field name="sequence">3</field>
    </record>
    <record id="tech_repair_state_pub_warranty" model="tech.repair.state.public">
        <field name="name">Warranty Processing</field>
        <field name="sequence">4</field>
    </record>
    <record id="tech_repair_state_pub_ready" model="tech.repair.state.public">
        <field name="name">Ready for Delivery</field>
        <field name="sequence">5</field>
    </record>
    <record id="tech_repair_state_pub_done" model="tech.repair.state.public">
        <field name="name">Delivered</field>
